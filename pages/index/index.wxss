.page_row {
  display: flex;
  flex-direction: row;
}

.search {
  width: 80%;
}

.search_edge {
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
  margin-left: 20rpx;
}

.searchcion {
  margin: 10rpx 10rpx 10rpx 10rpx;
  position: absolute;
  left: 25rpx;
  z-index: 2;
  width: 20px;
  height: 20px;
  text-align: center;
}

.search_edge input {
  margin-left: 60rpx;
  height: 60rpx;
  border-radius: 5px;
}

.sousuo {
  margin-left: 15rpx;
  width: 15%;
  line-height: 150%;
  text-align: center;
  border: 1px solid #d0d0d0;
  border-radius: 10rpx;
}

.img {
  width: 100%;
  height: 100%;
}

.nav, .swiper {
  width: 100%;
  border: 1px solid #d0d0d0;
}
